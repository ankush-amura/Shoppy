<fieldset>
  <legend>SignUp Desk</legend>
<p>
  <%= f.label :name %><br>
  <%= f.text_field :name %>
</p>

<p>
  <%= f.label :Email %><br>
  <%= f.text_field :email %>
</p>

<p>
  <%= f.label "Contact:" %><br>
  <%= f.number_field :contact %>
</p>

<p>
  <%= f.label :Password %><br>
  <%= f.password_field :password %>
</p>

<p>
  <%= f.label "Confirm Passsword:" %><br>
  <%= f.password_field :confirmPasssword %>
</p>

<p>
  <br>
  <%= f.submit "Submit Details" %>
  <br>
</p>

</fieldset>
